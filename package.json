{
  "name": "angular-cesium",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Angular2 library seed - based on Webpack",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "scripts": {
    "cleanup": "rimraf src/*.ngfactory.ts src/*.ngstyle.ts src/*.ngsummary.json build dist",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "ngc": "ngc -p tsconfig.aot.json",
    "aot": "webpack --config webpack.aot.config.js -p",
    "build": "npm run cleanup && npm run ngc",
    "build-demo": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "test-server": "node demo/performance-server/server.js",
    "compile-docs": "compodoc -p ./src/angular-cesium/tsconfig.json -n \"Angular 2 Cesium\"",
    "serve-docs": "compodoc -s",
    "compileNserve-docs": "npm run compile-docs && npm run serve-docs"
  },
  "private": true,
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@angular/core": "2.0.1"
  },
  "devDependencies": {
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
    "@angular/compiler-cli": "~2.3.0",
    "@angular/core": "~2.3.0",
    "@angular/forms": "~2.3.0",
    "@angular/http": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0",
    "@angular/router": "~3.3.0",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "bower": "^1.7.9",
    "browserify": "^13.1.1",
    "compodoc": "0.0.41",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "es6-module-loader": "^0.17.11",
    "es6-shim": "^0.35.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-symlink": "^2.1.4",
    "gulp-typescript": "^3.1.3",
    "gulp-uglify": "^1.5.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "merge2": "^1.0.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.3",
    "rimraf": "^2.5.2",
