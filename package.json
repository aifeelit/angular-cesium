{
  "name": "angular2-cesium",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Angular2 library for working with Cesium.",
  "repository": "https://github.com/TGFTech/angular2cesium",
  "keywords": [
    "Angular2",
    "Cesium",
    "Angular Cesium",
    "Angular2 Cesium"
  ],
  "homepage": "https://github.com/TGFTech/angular2cesium#readme",
  "main": "bundles/angular2-cesium.umd.js",
  "typings": "src/main.d.ts",
  "scripts": {
    "cleanup": "rimraf src/*.ngfactory.ts src/*.ngstyle.ts src/*.ngsummary.json build bundles",
    "start": "node_modules\\.bin\\webpack-dev-server --inline --progress --port 8080",
    "lint": "tslint -c tslint.json --project tsconfig.aot.json",
    "test": "karma start",
    "ngc": "node_modules\\.bin\\ngc -p tsconfig.aot.json",
    "aot": "node_modules\\.bin\\webpack --config webpack.aot.config.js -p",
    "bundle": "npm run cleanup && npm run lint && npm run ngc && npm run aot",
    "build": "rimraf dist && node_modules\\.bin\\webpack --config config/webpack.dev.js --progress --profile --bail",
    "test-server": "node demo/performance-server/server.js",
    "compile-docs": "compodoc src -p tsconfig.json -n \"Angular 2 Cesium\"",
    "serve-docs": "compodoc -s",
    "compileNserve-docs": "npm run compile-docs && npm run serve-docs"
  },
  "peerDependencies": {
    "@angular/core": "2.4.x"
  },
  "devDependencies": {
    "@angular/common": "~2.4.0",
