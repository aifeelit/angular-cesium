    "@types/jasmine": "2.5.46",
    "@types/node": "7.0.8",
    "@types/selenium-webdriver": "^3.0.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "bower": "^1.7.9",
    "browserify": "^14.1.0",
    "cesium": "~1.31.0",
    "codelyzer": "~2.0.0",
    "compodoc": "0.0.41",
    "concurrently": "^3.1.0",
