{
  "name": "angular-cesium",
  "version": "0.0.10",
  "license": "MIT",
  "description": "Angular library for working with Cesium.",
  "repository": "https://github.com/TGFTech/angular-cesium",
  "keywords": [
    "Angular2",
    "Cesium",
    "Angular Cesium",
    "Angular2 Cesium"
  ],
  "homepage": "https://github.com/TGFTech/angular-cesium#readme",
  "main": "bundles/angular-cesium.umd.js",
  "typings": "src/main.d.ts",
  "scripts": {
    "clean_src": "rimraf src/*.d.ts src/components/**/*.d.ts src/models/**/*.d.ts src/pipes/**/*.d.ts src/services/**/*.d.ts src/utils/**/*.d.ts src/**/*.metadata.json src/**/*.js src/**/*.js.map src/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.ngsummary.json",
    "cleanup": "npm run clean_src && rimraf index.js index.js.map index.d.ts index.metadata.json build bundles",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "lint": "tslint -c tslint.json --project tsconfig.aot.json",
    "test": "karma start",
    "ngc": "ngc -p tsconfig.aot.json",
    "aot": "webpack --config webpack.aot.config.js -p",
    "rollup": "rollup -cza",
    "bundle": "npm run cleanup && npm run lint && npm run ngc && npm run rollup",
    "build-demo": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "test-server": "node demo/performance-server/server.js",
    "server": "nodemon --exec ./node_modules/.bin/ts-node -- ./demo/server/src/main.ts",
    "compile-docs": "rimraf docs && compodoc -p src/tsconfig.compodoc.json -d docs -n \"Angular 2 Cesium\"",
    "serve-docs": "compodoc -s -d ./docs -r 4200",
    "compileNserve-docs": "npm run compile-docs && npm run serve-docs",
    "heroku-postbuild": "npm run build-demo"
  },
  "peerDependencies": {
    "@angular/common": "4.0.x",
    "@angular/core": "4.0.x"
  },
  "devDependencies": {
    "@angular/animations": "^4.1.1",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/compiler-cli": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/jasmine": "2.5.46",
    "@types/node": "^7.0.18",
    "@types/request-promise": "^4.1.33",
    "@types/selenium-webdriver": "^3.0.0",
    "@types/socket.io": "^1.4.29",
    "angular2-template-loader": "^0.6.0",
    "apollo-angular": "^0.13.0",
    "apollo-client": "^1.0.2",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "body-parser": "^1.17.1",
    "bower": "^1.7.9",
    "browserify": "^14.1.0",
    "casual": "^1.5.12",
    "cesium": "~1.31.0",
    "codelyzer": "~2.0.0",
    "compodoc": "0.0.41",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "css-loader": "^0.26.1",
    "del": "^2.2.0",
    "es6-module-loader": "^0.17.11",
    "es6-shim": "^0.35.2",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.1",
    "graphql": "^0.9.2",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^2.0.0",
    "graphql-tools": "^0.11.0",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.1",
    "merge2": "^1.0.2",
    "nodemon": "^1.11.0",
