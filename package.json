    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.10",
    "request-promise": "^4.2.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "run-sequence": "^1.2.1",
    "rxjs": "^5.1.0",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "style-loader": "^0.15.0",
    "ts-helpers": "^1.1.1",
    "ts-mockito": "1.2.0",
    "ts-node": "^3.0.2",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "^3.0.0",
    "webpack-node-externals": "^1.5.4",
    "expose-loader": "^0.7.3",
    "zone.js": "^0.8.4"
  },
  "dependencies": {
    "angular2parse": "^1.0.4",
    "dataloader": "^1.3.0",
    "geodesy": "^1.1.1",
    "json-string-mapper": "^1.0.0",
    "primitive-primitives": "0.0.5"
  }
}
